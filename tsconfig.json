{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "allowUnreachableCode": true,
    "outDir": "dist",
    "noImplicitAny": false,
    "strictNullChecks": true
  },
  "lib": ["es2015"],
  "exclude": ["node_modules"]
}
